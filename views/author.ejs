        <div class="right_frame">
          <a class="link_href" href="">
            <div class="icon-4">
              <img src="icon/Contrast.png" class="icon-2" alt="" />
            </div>
          </a>

          <div class="right_frame_btn_container">
            <a class="link_href" href="">
              <div class="icon-4">
                <img src="icon/Union.svg" alt="" />
              </div>
            </a> 
            <a class="link_href" href="">
              <div class="icon-4">
                <img src="icon/home.jpeg" alt="" />
              </div>
            </a>

            <a class="link_href" href="">
              <div class="icon-4">
                <img src="icon/service.jpeg" alt="" />
              </div>
            </a>

            <a class="link_href" href="">
